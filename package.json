{
  "private": true,
  "packageManager": "pnpm@7.18.1",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "license": "ISC",
  "author": "Hg",
  "scripts": {
    "cz": "czg",
    "dev": "pnpm -C play dev",
    "build": "pnpm run -C internal/build start",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "dev:docs": "pnpm -C docs dev",
    "build:docs": "pnpm -C docs  build",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "prepare": "husky install",
    "stub": "pnpm run -r --parallel stub",
    "postinstall": "pnpm run stub"
  },
  "peerDependencies": {
    "vue": "^3.2.36"
  },
  "dependencies": {
    "@h-material/components": "workspace:*",
    "@h-material/theme-chalk": "workspace:*",
    "@h-material/utils": "workspace:*",
    "html2canvas": "^1.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@esbuild-kit/cjs-loader": "^2.4.2",
    "@h-material/build": "workspace:^0.0.1",
    "@h-material/build-constants": "workspace:^0.0.1",
    "@h-material/build-utils": "workspace:*",
    "@h-material/constants": "workspace:*",
    "@h-material/eslint-config": "workspace:*",
    "@pnpm/types": "^8.9.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.10",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-clean-css": "^4.3.0",
    "@types/gulp-sass": "^5.0.0",
    "@types/sass": "^1.43.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "commitizen": "^4.2.6",
    "cz-git": "^1.4.1",
    "czg": "^1.4.1",
    "eslint": "^8.29.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^9.0.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "sass": "^1.56.2",
    "sucrase": "^3.29.0",
    "ts-morph": "^19.0.0",
    "typescript": "^4.9.4",
    "unplugin-vue-define-options": "^1.0.0",
    "unplugin-vue-macros": "^0.11.2",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^3.2.36"
  },
  "engines": {
    "node": ">= 16"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
